{
    "name": "pug-autocompile",
    "version": "1.0.0",
    "description": "Compile HTML from Pug with options and notifications",
    "keywords": [ "dopustim", "pug", "autocompile", "compile", "html", "template" ],
    "author": "DopustimVladimir",
    "license": "MIT",
    "repository": "https://github.com/dopustim/atom-pug-autocompile",
    "main": "./lib/main",
    "dependencies": {
        "js-beautify": "^1.14.0",
        "pug": "^3.0.2"
    },
    "devDependencies": {
        "@dopustim/eslint-config": "^1.1.0",
        "eslint": "^8.24.0"
    },
    "eslintConfig": {
        "extends": "@dopustim/eslint-config",
        "env": {
            "atomtest": true,
            "jasmine": true,
            "es2020": true,
            "node": true
        },
        "parserOptions": {
            "sourceType": "module",
            "ecmaVersion": 2020
        },
        "rules": {
            "max-len": [ 1, { "code": 100 } ]
        }
    },
    "engines": {
        "atom": ">=1.30.0 <2.0.0"
    },
    "consumedServices": {
        "status-bar": {
            "versions": {
                "^1.0.0": "consumeStatusBar"
            }
        }
    }
}
