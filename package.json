{
    "name": "pug-autocompile",
    "main": "./lib/pug-autocompile",
    "version": "0.1.0",
    "description": "Compile Pug files to HTML",
    "keywords": [
        "autocompile",
        "compile",
        "dopustim",
        "html",
        "pug",
        "template"
    ],
    "repository": "https://github.com/dopustim/atom-pug-autocompile",
    "license": "MIT",
    "engines": {
        "atom": ">=1.30.0 <2.0.0"
    },
    "consumedServices": {
        "status-bar": {
            "versions": {
                "^1.0.0": "consumeStatusBar"
            }
        }
    },
    "dependencies": {
        "js-beautify": "^1.14.0",
        "pug": "^3.0.2"
    },
    "devDependencies": {
        "@dopustim/eslint-config": "^1.0.0",
        "eslint": "^8.4.1"
    },
    "eslintConfig": {
        "extends": "@dopustim/eslint-config",
        "env": {
            "atomtest": true,
            "jasmine": true,
            "es2020": true,
            "node": true
        },
        "parserOptions": {
            "sourceType": "module",
            "ecmaVersion": 2020
        },
        "rules": {
            "max-len": [ 1, { "code": 100 } ]
        }
    }
}
